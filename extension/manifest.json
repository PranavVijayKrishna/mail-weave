{
    "maifest version": 3,
    "name": "MailWeave",
    "version": "1.0.0",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "https://mail.google.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://mail.google.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}

